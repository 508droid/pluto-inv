"VertexLitGeneric"
{
	"$basetexture" "models/detroit/markus/eyeball_warning"
	"$nocull" 1
	 $selfillum 1
	$selfillummask "color/white"

	 

	"$surfaceprop" "Default"
	"Proxies"
{
	"AnimatedTexture"
{
	"animatedTextureVar" "$basetexture"
	"animatedTextureFrameNumVar" "$frame"
	"animatedTextureFrameRate" "2"
}
} 

	$envmap "env_cubemap"
	$envmapcontrast 1
	$envmaptint "[0.4 0 0]"


}